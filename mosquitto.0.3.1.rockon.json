{
    "Mosquitto": {
        "description": "Eclipse Mosquitto An open source MQTT broker. Version 1.6.12",
        "version": "0.3.1",
        "website": "https://mosquitto.org",
        "icon": "https://mosquitto.org/images/mosquitto-text-side-28.png",
        "more_info": "https://mosquitto.org/man/mosquitto-8.html",
        "volume_add_support": true,
        "containers": {
            "mosquitto_service": {
                "image": "eclipse-mosquitto",
                "tag": "1.6.12",
                "launch_order": "1",
                "ports": {
                    "1883": {
                        "description": "Used for MQTT (1883)",
                        "label": "MQTT",
                        "host_default": 1883
                    },
                    "9001": {
                        "description": "Used for WebUI? (9001)",
                        "label": "WebUI?",
                        "host_default": 9001
                    }
                },
                "volumes": {
                    "/mosquitto/": {
                        "description": "Configurations, Data and Logs for Mosquitto (ie. mosquitto.conf)",
                        "label": "Mosquitto Persistence",
                        "min_size": 1000
                    }
                }
            }
        }
    }
}
